    "require": {
        "php": ">=5.5.9",
        "ext-dom": "*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*",
        "doctrine/dbal": "^2.5",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
